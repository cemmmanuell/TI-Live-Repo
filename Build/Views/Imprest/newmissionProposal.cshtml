<script>
    function insertproporsal100000() {

        var objectives = [];
        var focus;
        var outcome; var roles = []; var activities = []; var budgetinfo = []; var background; var budgetnotes = [];
        debugger;

        focus = $('#itstrategicfocus').val();
        outcome = $('#missionoutcome').val();
        background = $('#background').val();

        var table1, table2, table3, table4, table5, table6;

        table1 = $('#msobjectives tr');
        table2 = $('#table2 tr');
        table3 = $('#table3 tr');
        table4 = $('#table4 tr');
        table5 = $('#table5 tr');
        table6 = $('#table6 tr');
        table7 = $('#table7 tr');
        table1.each(function () {
            var row = $(this);
            //var Statement = {};
            var rowdata = row.find("td:eq(0) input[type='text']").val();

            objectives.push(rowdata);
        });
      table4.each(function () {
           var row = $(this);
           //var Statement = {};
           var rowdata = row.find("td:eq(0) input[type='text']").val() + '??' + row.find("td:eq(1) input[type='text']").val();

           roles.push(rowdata);
       }); table5.each(function () {
           var row = $(this);
           //var Statement = {};
           var rowdata = row.find("td:eq(0) input[type='Date']").val() + '??' + row.find("td:eq(1) input[type='text']").val() + '??'+ row.find("td:eq(2) input[type='number']").val() + '??' + row.find("td:eq(3) input[type='text']").val();

           activities.push(rowdata);
       }); table6.each(function () {
           var row = $(this);
           //var Statement = {};
           var rowdata = row.find("td:eq(0) input[type='text']").val() + '??' + row.find("td:eq(1) input[type='text']").val() + '??' + row.find("td:eq(2) input[type='number']").val() + '??' + row.find("td:eq(3) input[type='number']").val() + '??' + row.find("td:eq(4) input[type='number']").val() + '??' + row.find("td:eq(5) select option:selected").val() + '??' + row.find("td:eq(8) select option:selected").val();

           budgetinfo.push(rowdata);
       });

       table7.each(function () {
           var row = $(this);
           //var Statement = {};
           var rowdata = row.find("td:eq(0) input[type='text']").val() + '??' + row.find("td:eq(1) input[type='text']").val() + '??' + row.find("td:eq(3) input[type='text']").val() + '??' + row.find("td:eq(4) input[type='text']").val() + '??' + row.find("td:eq(4) input[type='number']").val() + '??';

           budgetnotes.push(rowdata);
       });

        $.ajax({
            type: "POST",
            url: "/Imprest/insertmissionproposal",
            data:JSON.stringify( { strategicfocusarea: $('#strategicfocusarea10 option:selected').val(),
                 subpillar: $('#subpillar10 option:selected').val(), projecttitle:
               $('#projectittle10').val(),country: $('#country10 option:selected').val(),
               county: $('#county10 option:selected').val(),dateofactivities:
                $('#dateofactivity10').val() ,missionteam: $('#missionteam10').val(),
               invitedmembers:  $('#invitedmembers10').val(), fundcode:
                 $('#department10 option:selected').val() , programcode: $('#programcode10 option:selected').val()
                ,budgetdimension: $('#budgetdimension option:selected').val(),
                departmentdimension: $('#departmentdimesion option:selected').val()
               ,budgetdescription:  $('#budgetdescription option:selected').val(),
                objectives: objectives , focus: focus
               , outcome: outcome, roles: roles, activities: activities, budgetinfo: budgetinfo, background: background, budgetnotes: budgetnotes
            }),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (response) {
                /// alert("You selected: " + response.AuthSuccess);
                if (response.status == true) {
                    alert('Saved succesfuly');

                    window.location.href = '/Imprest/openMissionProposals';

                } else {
                    alert(resonse.message);



                }


<script>
    function insertimpressurrender() {
     
      //  alert(implines);
        debugger;
        $.ajax({
            type: "POST",
            url: "/Imprestaccounting/insertimprestsurrender",
            data: '{  imprestno:"' + $('#missionno30').val() + '" }',
            contentType: "application/json; charset=utf-8",
            dataType: "html",
            success: function (response) {
                $('#surrenderboy').html('');
                $('#surrenderboy').html(response);
            }

        });
    }

    function addline() {

        var tblstring = '<tr><td>' + $('#type option:selected').val() + '</td><td>' + $('#type option:selected').text() + '</td><td>' + $('#imprestamount').val() + '</td><td><button type="button" onclick="removeline()" class="btn bg-blue btn-sm legitRipple"><i class="icon-checkmark3 mr-2">Remove</button></td></tr>';
        $('#tbodyimprestline').html('');
        $('#tbodyimprestline').append(tblstring)

    }

    function html2json() {
        var json = '{';
        var otArr = [];
        var tbl2 = $('#tbodyimprestline tr').each(function (i) {
            x = $(this).children();
            var itArr = [];
            x.each(function () {
                itArr.push('"' + $(this).text() + '"');
            });
            otArr.push('"' + i + '": [' + itArr.join(',') + ']');
        })
        json += otArr.join(",") + '}'

        return json;
    }

</script>